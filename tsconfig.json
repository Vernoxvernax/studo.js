{
  "compilerOptions": {
    "target": "ES2019",
    "sourceMap": false,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "strict": true,
    "exactOptionalPropertyTypes": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitOverride": true,
    "declaration": false,
    "pretty": false,
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
